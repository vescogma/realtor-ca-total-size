{
  "name": "Realtor.ca Tools",
  "description": "Get the home size level breakdown from a realtor.ca listing page.",
  "version": "1.2",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon@0.5x.png",
      "32": "/images/icon@1x.png",
      "48": "/images/icon@1.5x.png",
      "64": "/images/icon@2x.png",
      "128": "/images/icon@4x.png"
    }
  },
  "icons": {
    "16": "/images/icon@0.5x.png",
    "32": "/images/icon@1x.png",
    "48": "/images/icon@1.5x.png",
    "64": "/images/icon@2x.png",
    "128": "/images/icon@4x.png"
  }
}
